{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "target": "es2017",
    "isolatedModules": false,
    "noEmit": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": ["server/**/*.ts"]
}